  "paths": {
    "/vehicles/{vin}/status": {
      "get": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Retrieve the status for a Volvo vehicle",
        "operationId": "getStateByVehicleId",
        "parameters": [
          {
            "name": "vin",
            "in": "path",
            "description": "The vin which is needed to retrieve the status",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/VolvoStatusResponse"
                }
              }
            }
          }
        }
      }
    },
    "/vehicles/{vin}/position": {
      "get": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Retrieves the current position for a Volvo vehicle",
        "operationId": "getPositionByVehicleId",
        "parameters": [
          {
            "name": "vin",
            "in": "path",
            "description": "The vin which is needed to retrieve the current position",
