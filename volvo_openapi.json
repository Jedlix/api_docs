          {
            "name": "vin",
            "in": "path",
            "description": "The vin which is needed to retrieve the charge locations",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/VolvoChargeLocationsResponse"
                }
              }
            }
          }
        }
      },
      "put": {
        "tags": [
          "Vehicle"
        ],
        "summary": "Update the charge locations for a Volvo vehicle",
        "operationId": "postNewChargeSettings",
        "parameters": [
          {
            "name": "vin",
            "in": "path",
            "description": "The vin which is needed to update the charge locations",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "later",
          "content": {
            "*/*": {
              "schema": {
                "$ref": "#/components/schemas/VolvoPutLocationRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/VolvoChargeLocationsResponse"
                }
              }
            }
          }
        },
        "x-codegen-request-body-name": "chargeLocationModel"
      }
    }
